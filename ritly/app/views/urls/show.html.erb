

<div class ="container show_all">
	
	<h5> Copy the following URL in your browser </h5>

	
		<table class="table table-hover" >
			<tr>
				<td><strong>localhost:3000/go/<%= @url.random_string %> </strong></td>
				<td><%= link_to("Preview", preview_path(@url.random_string))%></td>
				
				<td><%=  link_to 'Edit', edit_url_path(@url[:id]) %></td>
				<td><%= link_to "Delete", @url, :method => :delete %></td>
			</tr>
		</table>
		
</div>



